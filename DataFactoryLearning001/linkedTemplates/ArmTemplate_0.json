				"activities": [
					{
						"name": "Wait for 10 mins",
						"type": "Wait",
						"dependsOn": [],
						"userProperties": [
							{
								"name": "createdBy",
								"value": "SurendraKothapalli"
							}
						],
						"typeProperties": {
							"waitTimeInSeconds": 10
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": []
		},
		{
