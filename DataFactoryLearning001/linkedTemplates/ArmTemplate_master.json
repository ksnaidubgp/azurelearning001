{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactoryLearning001"
		},
		"LS_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage'"
		},
		"LS_Blobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Blobstorage'"
		},
		"LS_SQLDatabase001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLDatabase001'"
		},
		"LS_ADLSGen2_KeyVault_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azlearningadls001.dfs.core.windows.net/"
		},
		"LS_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azlearningadls001.dfs.core.windows.net/"
		},
		"LS_BlobStroage_Keyvault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "azureBlob-ConnectionString"
		},
		"LS_HTTPConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().baseURL}"
		},
		"LS_KeyVault001_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azureKVSurendra.vault.azure.net/"
		},
		"LS_SQLDB001_Keyvault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "azuresqldb001-ConnectionString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactoryLearning001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_accountKey')]"
					},
					"LS_Blobstorage_connectionString": {
						"value": "[parameters('LS_Blobstorage_connectionString')]"
					},
					"LS_SQLDatabase001_connectionString": {
						"value": "[parameters('LS_SQLDatabase001_connectionString')]"
					},
					"LS_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"LS_HTTPConnection_properties_typeProperties_url": {
						"value": "[parameters('LS_HTTPConnection_properties_typeProperties_url')]"
					},
					"LS_KeyVault001_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault001_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLSGen2_KeyVault_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGen2_KeyVault_properties_typeProperties_url')]"
					},
					"LS_BlobStroage_Keyvault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BlobStroage_Keyvault_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQLDB001_Keyvault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQLDB001_Keyvault_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DataFactoryLearning001_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DataFactoryLearning001_ArmTemplate_0')]"
			]
		}
	]
}